{
  "name": "khan-zia/laravel-saml-idp",
  "description": "Make your Laravel application an Identity Provider using SAML 2.0.",
  "keywords": [
    "laravel",
    "saml",
    "saml 2.0",
    "auth",
    "acl",
    "sso",
    "idp"
  ],
  "license": "proprietary",
  "require": {
    "php": "^7.3",
    "illuminate/support": "^7.0|^8.0",
    "illuminate/routing": "^7.0|^8.0",
    "illuminate/database": "^7.0|^8.0",
    "simplesamlphp/saml2": "4.0"
  },
  "autoload": {
    "psr-4": {
      "ZiaKhan\\SamlIdp\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "ZiaKhan\\SamlIdp\\SamlIdpServiceProvider"
      ]
    }
  }
}
